{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "prefer-arrow-callback": "warn",
    "consistent-return": "off",
    "func-names": ["error", "never"],
    "prefer-destructuring": "off",
    "no-shadow": "off",
    "global-require": "off",
    "no-underscore-dangle": "off",
    "react/destructuring-assignment": "off"
  }
}
